{
  "name": "katie-token",
  "version": "1.0.0",
  "description": "erc-721 katie token",
  "author": "Min Ju<route666@live.cn>",
  "license": "MIT",
  "main": "dest",
  "types": "dest",
  "files": [
    "dest",
    "build"
  ],
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "prepare": "tsc",
    "postinstall": "truffle compile && yarn generate-types",
    "clean": "rm -rf dest node_modules",
    "ganache": "ganache-cli --quiet --networkId $npm_package_config_ganache_networkId --allowUnlimitedContractSize --gasLimit $npm_package_config_ganache_gasLimit --gasPrice $npm_package_config_ganache_gasPrice --mnemonic \"$MNEMONIC\"",
    "compile": "truffle compile",
    "test": "concurrently \"yarn ganache\" \"yarn compile && tsc && truffle test ./dest/*.test.js\"",
    "deploy:testnet": "truffle deploy --network ropsten"
  },
  "config": {
    "ganache": {
      "networkId": "*",
      "gasPrice": 25000000000,
      "gasLimit": 8000000
    }
  },
  "repository": "git+https://github.com/ilovelili/KatieToken.git",
  "keywords": [
    "erc-721",
    "token"
  ],
  "dependencies": {},
  "devDependencies": {
    "@openzeppelin/contracts": "^3.1.0",
    "@truffle/hdwallet-provider": "^1.0.36",
    "@typechain/truffle-v5": "^2.0.2",
    "@types/node": "^14.0.14",
    "dotenv": "^8.2.0",
    "ganache-cli": "^6.9.1",
    "truffle": "^5.1.32",
    "ts-node": "^8.10.2",
    "typechain": "^2.0.0",
    "typescript": "^3.9.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^7.0.2",
    "concurrently": "^5.2.0"
  }
}